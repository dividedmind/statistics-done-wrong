title = {{Discrepancies in sample size calculations and data analyses reported in randomised trials: comparison of publications with protocols}},
journal = {BMJ},
year = {2008},
volume = {337},
pages = {a2299},
affiliation = {Mayo Clinic, Rochester, USA. chan.anwen@mayo.edu},
